// Witz programming language

@dog {
	$name:str;
	$fleas:stack;
	#dog(name:str){
		this.name = name;
		this.fleas.push('pepe');
		this.fleas.push('juan');
	}
	#eat:int(food:Food) {
		food.consume();
		<- new poop;
	}
	#scratch:void() {
		while(this.fleas.pop()){
			print('scratch scratch');
		}
	}
}

@pug <dog>{
	#snort:void() {
		$i = 5*1-4+4/1%5;
		for i -> 0 skip -1 {
			print('snort snort');
		}
	}
}

$name = input('Name your dog: ');
$mydog:dog = new dog(name);
mydog.scratch();
